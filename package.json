{
  "name": "puro",
  "version": "1.2.11",
  "description": "Puro is a versatile and dynamic Discord bot designed to enhance your server experience. With a wide range of commands and seamless integration, Puro helps automate tasks and bring more fun and efficiency to your Discord community.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --quiet --watch . --ignore database.json --ignore '*.log' --trace-warnings app.js",
    "dev:debug": "nodemon --inspect --verbose --watch . --ignore database.json --ignore '*.log' --trace-warnings app.js",
    "lint": "eslint . --ext .js --fix",
    "lint:check": "eslint . --ext .js",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "validate": "npm run lint:check && npm test"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@vladfrangu/async_event_emitter": "2.4.8-1.0",
    "axios": "^1.13.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "js-yaml": "^4.1.1",
    "@discordjs/voice": "^0.17.0",
    "prism-media": "^1.3.5"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11"
  },
  "keywords": [
    "discord",
    "bot",
    "automation",
    "community",
    "fun",
    "discord.js",
    "slash-commands"
  ],
  "author": "MaxUltimat3",
  "company": "Skypixel",
  "license": "ISC",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norbertbaricz/Puro"
  },
  "bugs": {
    "url": "https://github.com/norbertbaricz/Puro/issues"
  }
}
