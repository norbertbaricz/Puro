{
  "name": "puro",
  "version": "1.1.6",
  "description": "Puro is a versatile and dynamic Discord bot designed to enhance your server experience. With a wide range of commands and seamless integration, Puro helps automate tasks and bring more fun and efficiency to your Discord community.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --quiet --watch . --ignore database.json --trace-warnings app.js",
    "dev": "nodemon --inspect --verbose --watch . --ignore database.json --trace-warnings app.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "axios": "^1.9.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "ffmpeg-static": "^5.2.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "discord",
    "bot",
    "automation",
    "community",
    "fun",
    "discord.js",
    "slash-commands"
  ],
  "author": "Norbert Baricz",
  "license": "ISC",
  "engines": {
    "node": ">=16.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norbertbaricz/Puro"
  },
  "bugs": {
    "url": "https://github.com/norbertbaricz/Puro/issues"
  }
